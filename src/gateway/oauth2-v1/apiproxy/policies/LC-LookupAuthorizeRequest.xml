<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<LookupCache async="false" continueOnError="false" enabled="true" name="LC-LookupAuthorizeRequest">
    <DisplayName>LC-LookupAuthorizeRequest</DisplayName>
    <CacheKey>
        <KeyFragment>authorize_request</KeyFragment>
        <KeyFragment ref="flow.cache.key"/>
    </CacheKey>
    <Scope>Global</Scope>
    <CacheResource>oauth2-v1${proxy.name.suffix}</CacheResource>
    <CacheLookupTimeoutInSeconds>10</CacheLookupTimeoutInSeconds>
    <AssignTo>flow.authorize_request</AssignTo>
</LookupCache>